<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD config 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/> <!-- date에 널 허용 -->
	</settings>
	<typeAliases>
		<typeAlias type="com.google.pronect.vo.Member" alias="Member"/>
		<typeAlias type="com.google.pronect.vo.Group" alias="Group"/>
		<typeAlias type="com.google.pronect.vo.GComment" alias="GComment"/>
		<typeAlias type="com.google.pronect.vo.Fboard" alias="Fboard"/>
		<typeAlias type="com.google.pronect.vo.Fcomment" alias="Fcomment"/>
		<typeAlias type="com.google.pronect.vo.Qboard" alias="Qboard"/>
		<typeAlias type="com.google.pronect.vo.Schedule" alias="Schedule"/>
		<typeAlias type="com.google.pronect.vo.Achive" alias="Achive"/>
		<typeAlias type="com.google.pronect.vo.GroupBoard" alias="GroupBoard"/>
		<typeAlias type="com.google.pronect.vo.GroupBoardReply" alias="GroupBoardReply"/>
	</typeAliases>
	<mappers>
		<mapper resource="META-INF/sqlmap/mappers/member.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/group.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/g.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/gcomment.xml"/>
		<!-- <mapper resource="META-INF/sqlmap/mappers/pcomment.xml"/> -->
		<mapper resource="META-INF/sqlmap/mappers/fboard.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/fcomment.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/qboard.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/schedule.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/achive.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/groupboard.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/groupboardreply.xml"/>
	</mappers>
</configuration>